{
    "compilerOptions": {
        "typeRoots": ["src/typings", "node_modules/@types"],
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es6",
        "allowJs": true,
        "moduleResolution": "node",
        "strictNullChecks": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "baseUrl": ".",
        "paths": {
            "*": ["src/*"]
        }
    }
}
